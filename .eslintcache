e.log(error))\n        }\n        if (trailerUrl) {\n                setTrailerUrl(\"\");\n                console.log(trailerUrl, \"is empty\")\n        } else {\n            AXIOS.get(`https://api.themoviedb.org/3/movie/${movie.id}?api_key=${API_KEY}&append_to_response=videos`)\n                .then((res)=>{\n\n                    const trailer = res.data.videos.results\n                    if(trailer.length !== 0) {\n                        trailer.map(videos => {\n                            setTrailerUrl(videos.key)\n                        })\n                    }\n                }).catch(error => console.log(error))\n                \n        }\n      \n\n    }\n\n\n\n    return (\n        <div className=\"row\">\n            <h2>{title}</h2>\n\n            <div className=\"row__posters\">  \n\n            {movies.map((movie) => (\n                <img \n                key={movie.id}\n                onClick = {() => handleClick(movie)}\n                className= {`row__poster ${isLargeRow && \"row__posterLarge\"}`}\n                src={`${baseURL}${isLargeRow ? movie.poster_path : movie.backdrop_path}`} \n                alt={movie.name}/>\n            ))}\n            </div>\n            {trailerUrl && < Youtube videoId={trailerUrl} opts={opts} />}\n\n        </div>\n    )\n}\n\nexport default Row\n","/Users/alirizvi/Desktop/sei-projects/personal-project/personalProjectsNew/netflixClone/fe-netflix/src/axios.js",[],"/Users/alirizvi/Desktop/sei-projects/personal-project/personalProjectsNew/netflixClone/fe-netflix/src/requests.js",[],"/Users/alirizvi/Desktop/sei-projects/personal-project/personalProjectsNew/netflixClone/fe-netflix/src/Banner.js",[],"/Users/alirizvi/Desktop/sei-projects/personal-project/personalProjectsNew/netflixClone/fe-netflix/src/Navbar.js",["65"],"/Users/alirizvi/Desktop/sei-projects/personal-project/personalProjectsNew/netflixClone/fe-netflix/src/SignUp.js",[],["66","67"],"/Users/alirizvi/Desktop/sei-projects/personal-project/person